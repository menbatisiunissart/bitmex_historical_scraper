    "version": "0.2.0",
    "configurations": [
        {
            "name": "Scrape all",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "justMyCode": false
        },
        {
            "name": "Scrape from 2016",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "args": [
                "--start",
                "20160101",
                "--end",
                "20171122"
            ],
            "justMyCode": false
        },
        {
            "name": "Scrape from start through end date",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "args": [
                "--start",
                "20141122",
                "--end",
                "20151122"
            ],
            "justMyCode": false
        }
    ]
}