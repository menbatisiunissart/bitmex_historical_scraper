    "version": "0.2.0",
    "configurations": [
        {
            "name": "Scrape all",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
        },
        {
            "name": "Scrape 2016 XBTUSD trade",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "args": [
                "--start",
                "20160101",
                "--end",
                "20160331",
                "--symbol",
                "XBTUSD"
            ]
        },
        {
            "name": "Scrape 2016 XBTUSD quote",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "args": [
                "--start",
                "20160101",
                "--end",
                "20160331",
                "--channel",
                "quote",
                "--symbol",
                "XBTUSD"
            ]
        },
        {
            "name": "Scrape from start through end date",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scrape.py",
            "args": [
                "--start",
                "20141122",
                "--end",
                "20151122"
            ]
        }
    ]
}